  "ApiDocsData": {
    "categories": {
      "projects": {
        "name": "Projects",
        "description": "Project related API endpoints",
        "endpoints": {
          "getProjects": {
            "description": "Retrieve all projects list.",
            "responses": {
              "200": {
                "description": "Projects list successfully returned.",
                "example": {
                  "0": {
                    "name": "Sample Project",
                    "description": "This is a sample project."
                  }
                }
              },
              "401": {
                "description": "Authentication failed",
                "example": {
                  "message": "Authentication failed: Invalid API key"
                }
              },
              "500": {
                "description": "Server error",
                "example": {
                  "message": "An error occurred while retrieving projects list."
                }
              }
            }
          },
          "createProject": {
            "description": "Create a new project.",
            "requestBody": {
              "properties": {
                "name": {
                  "description": "Project name"
                },
                "description": {
                  "description": "Project description"
                }
              }
            },
            "responses": {
              "201": {
                "description": "Project successfully created.",
                "example": {
                  "name": "New Project",
                  "description": "New project description"
                }
              },
              "400": {
                "description": "Bad request",
                "example": {
                  "message": "Project name is required"
                }
              },
              "401": {
                "description": "Authentication failed",
                "example": {
                  "message": "Authentication failed: Invalid API key"
                }
              },
              "500": {
                "description": "Server error",
                "example": {
                  "message": "An error occurred while creating the project"
                }
              }
            }
          },
          "getProjectById": {
            "description": "Retrieve a specific project by its ID.",
            "parameters": {
              "projectId": {
                "description": "ID of the project to retrieve"
              }
            },
            "responses": {
              "200": {
                "description": "Project successfully returned.",
                "example": {
                  "name": "Sample Project",
                  "description": "This is a sample project.",
                  "images": {
                    "0": {
                      "name": "Sample Image"
                    }
                  }
                }
              },
              "401": {
                "description": "Authentication failed",
                "example": {
                  "message": "Authentication failed: Invalid API key"
                }
              },
              "404": {
                "description": "Project not found",
                "example": {
                  "message": "Project not found"
                }
              },
              "500": {
                "description": "Server error",
                "example": {
                  "message": "Error fetching project"
                }
              }
            }
          },
          "deleteProjectById": {
            "description": "Delete a specific project by its ID.",
            "parameters": {
              "projectId": {
                "description": "ID of the project to delete"
              }
            },
            "responses": {
              "200": {
                "description": "Project successfully deleted.",
                "example": {
                  "message": "Project successfully deleted."
                }
              },
              "401": {
                "description": "Authentication failed",
                "example": {
                  "message": "Authentication failed: Invalid API key"
                }
              },
              "404": {
                "description": "Project not found",
                "example": {
                  "message": "Project not found"
                }
              },
              "500": {
                "description": "Server error",
                "example": {
                  "message": "Error deleting project"
                }
              }
            }
          }
        }
      },
      "images": {
        "name": "Images",
        "description": "Image related API endpoints",
        "endpoints": {
          "uploadImage": {
            "description": "Upload a new image to a specific project and save it with specified format/size conversions.",
            "parameters": {
              "projectId": {
                "description": "ID of the project to upload the image to"
              }
            },
            "requestBody": {
              "properties": {
                "file": {
                  "description": "Image file to upload"
                },
                "variants": {
                  "description": "Image version options to create. Each element is of the form '{' \"sizeLabel\": \"original\" | \"300x300\" | \"800x800\" | \"1920x1080\", \"format\": \"png\" | \"jpeg\" | \"avif\" | \"webp\" '}'"
                },
                "saveOriginal": {
                  "description": "\"true\" or \"false\". Whether to save the original image (default: true)"
                }
              }
            },
            "responses": {
              "201": {
                "description": "On success of all version processing"
              },
              "207": {
                "description": "On partial failure of version processing",
                "example": {
                  "message": "Partial image conversion processing failure",
                  "errors": {
                    "0": {
                      "error": "An error occurred during image conversion processing."
                    }
                  }
                }
              },
              "400": {
                "description": "Bad request",
                "example": {
                  "message": "Required parameters are missing."
                }
              },
              "401": {
                "description": "Authentication failed",
                "example": {
                  "message": "Authentication failed: Invalid API key"
                }
              },
              "404": {
                "description": "Project not found",
                "example": {
                  "message": "Project not found."
                }
              },
              "415": {
                "description": "Unsupported media type",
                "example": {
                  "message": "Content-Type is not multipart/form-data."
                }
              },
              "500": {
                "description": "Server error",
                "example": {
                  "message": "An error occurred while processing the image."
                }
              }
            }
          },
          "deleteImage": {
            "description": "Delete an image with a specific ID and all related version files (including OCI storage).",
            "parameters": {
              "projectId": {
                "description": "ID of the project the image belongs to (for path validation)"
              },
              "imageId": {
                "description": "ID of the image to delete"
              }
            },
            "responses": {
              "200": {
                "description": "Image deletion successful",
                "example": {
                  "message": "Image deleted successfully"
                }
              },
              "400": {
                "description": "Bad request",
                "example": {
                  "message": "imageId is missing"
                }
              },
              "401": {
                "description": "Authentication failed",
                "example": {
                  "message": "Authentication failed: Invalid API key"
                }
              },
              "404": {
                "description": "Image not found",
                "example": {
                  "message": "Image with the given imageId not found or projectId mismatch."
                }
              },
              "500": {
                "description": "Server error",
                "example": {
                  "message": "An error occurred while deleting the image."
                }
              }
            }
          }
        }
      }
    }
  }
